      <ownedComment xmi:type="uml:Comment" xmi:id="_A6j2UK6KEfCy7LlARt0Vrg">
        <body>@startuml ActivityDiagram_ManageEvents

title Activity Diagram: Manage Events

' Define the starting point of the flow
start

' Define the Actor (Admin) and the System Swimlanes
partition &quot;USU Administrator&quot; as Admin {
  :Select Option to Create, Modify, or Delete Event;
  
  ' Decision node for authorization (RBAC)
  if (Is Administrator Authorized?) then (Yes)
    :Input Event Details / Apply Changes;
    
    ' Transition to the System Partition
    -> [Submit Action] System;
  else (No)
    :Display &quot;Authorization Failed&quot; Message;
    stop
  endif
}

partition &quot;USU OS System&quot; as System {
  
  ' Fork: Parallel processing for data handling and logging
  fork
    :Apply Event Changes to Database;
    
    ' Activity corresponding to the &lt;&lt;include>> Encryption
    :Encrypt Event Data for Storage and Transmission;
    
    ' Decision node for required synchronization
    if (Update Requires Synchronization?) then (Yes)
      :Propagate Updates to Relevant Interfaces (Synchronization);
    else (No)
    endif
    
    :Confirm Event Saved Successfully;
  fork again
    ' Activity corresponding to the &lt;&lt;include>> Logs
    :Generate Audit Log of Administrator Action (Log Officer Action);
    
    :Export Log in JSON/CVS format;
  end fork
  
  :Event Management Complete;
  
  ' Transition back to the Admin Partition
  -> [Result] Admin;
}

partition &quot;USU Administrator&quot; {
  :Display Success Message / Updated Event List;
}

' Define the ending point of the flow
stop

@enduml</body>
      </ownedComment>
